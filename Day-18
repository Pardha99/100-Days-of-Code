class Solution {
public:
    int mySqrt(int x) {
        
        if(x == 0) return 0;
        if(x == 1) return 1;
        int ans =0 ;
        //
        for(long long int  i=1;i<x;i++){

            if(i*i <= x){
                ans = i;
            }else{
                break;
            }
        }
        return ans;

    }
};
